{
  "name": "storysign-mobile",
  "version": "1.0.0",
  "description": "StorySign ASL Platform Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace StorySignMobile.xcworkspace -scheme StorySignMobile -configuration Release -destination generic/platform=iOS -archivePath StorySignMobile.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-vector-icons": "^10.0.3",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-webrtc": "^118.0.7",
    "react-native-camera": "^4.2.1",
    "@react-native-camera/camera": "^7.1.0",
    "react-native-permissions": "^4.1.5",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "react-native-background-sync": "^1.0.1",
    "react-native-netinfo": "^11.2.1",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-device-info": "^10.13.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-keychain": "^8.2.0",
    "react-native-biometrics": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
