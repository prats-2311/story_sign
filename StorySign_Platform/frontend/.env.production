# Production Environment Variables for StorySign Frontend
# These are the default production settings
# Override in Netlify environment variables as needed

# API Configuration (will be overridden by Netlify env vars)
REACT_APP_API_URL=https://storysign-backend.onrender.com
REACT_APP_WS_URL=wss://storysign-backend.onrender.com
REACT_APP_API_VERSION=v1

# Deployment Configuration
REACT_APP_USE_PROXY=true
REACT_APP_ENVIRONMENT=production

# Build Optimization
GENERATE_SOURCEMAP=false
CI=true

# Performance Optimization
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0